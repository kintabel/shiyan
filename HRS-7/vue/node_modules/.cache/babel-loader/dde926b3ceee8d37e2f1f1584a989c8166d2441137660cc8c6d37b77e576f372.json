{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'TodayAppointments',\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      // 当前用户信息\n      appointments: [],\n      // 挂号信息列表\n      loading: false,\n      // 加载状态\n      error: '',\n      // 错误信息\n      currentPage: 1,\n      // 当前页码\n      pageSize: 10,\n      // 每页显示条数\n      totalAppointments: 0 // 总条数\n    };\n  },\n\n  created() {\n    this.fetchTodayAppointments(1); // 初始化加载今日预约信息\n  },\n\n  methods: {\n    // 获取今日预约信息\n    fetchTodayAppointments(pageNumber) {\n      this.loading = true;\n      this.error = '';\n      this.currentPage = pageNumber;\n      this.$request.get('/doctor/selectAppointments', {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          userId: this.user.id // 传递当前用户 ID\n        }\n      }).then(response => {\n        if (response.data && response.data.list) {\n          this.appointments = response.data.list;\n        }\n        if (response.data && response.data.total) {\n          this.totalAppointments = response.data.total;\n        }\n      }).catch(error => {\n        console.error('获取今日预约信息失败:', error);\n        this.error = '获取今日预约信息失败，请稍后重试';\n      }).finally(() => {\n        this.loading = false;\n      });\n    },\n    // 分页切换\n    handlePageChange(newPage) {\n      this.fetchTodayAppointments(newPage);\n    },\n    // 查看挂号详情\n    handleViewDetails(row) {\n      this.$router.push({\n        name: 'RegistrationDetail',\n        params: {\n          id: row.id\n        } // 传递挂号 ID\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","user","JSON","parse","localStorage","getItem","appointments","loading","error","currentPage","pageSize","totalAppointments","created","fetchTodayAppointments","methods","pageNumber","$request","get","params","pageNum","userId","id","then","response","list","total","catch","console","finally","handlePageChange","newPage","handleViewDetails","row","$router","push"],"sources":["src/views/doctor/TodayAppointments.vue"],"sourcesContent":["<template>\r\n  <div class=\"today-appointments\">\r\n    <h2>今日预约</h2>\r\n\r\n    <!-- 加载状态 -->\r\n    <div v-if=\"loading\" class=\"loading\">加载中...</div>\r\n\r\n    <!-- 错误提示 -->\r\n    <div v-if=\"error\" class=\"error\">{{ error }}</div>\r\n\r\n    <!-- 挂号信息表格 -->\r\n    <el-table v-if=\"appointments.length > 0\" :data=\"appointments\" stripe>\r\n      <el-table-column prop=\"patientName\" label=\"患者姓名\"></el-table-column>\r\n      <el-table-column prop=\"appointmentTime\" label=\"预约时间\"></el-table-column>\r\n      <el-table-column prop=\"status\" label=\"状态\"></el-table-column>\r\n      <el-table-column label=\"操作\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"primary\" size=\"small\" @click=\"handleViewDetails(scope.row)\">查看详情</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!-- 无数据提示 -->\r\n    <div v-else class=\"no-data\">今日暂无预约信息</div>\r\n\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n        background\r\n        layout=\"prev, pager, next\"\r\n        :current-page=\"currentPage\"\r\n        :page-size=\"pageSize\"\r\n        :total=\"totalAppointments\"\r\n        @current-change=\"handlePageChange\"\r\n    ></el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'TodayAppointments',\r\n  data() {\r\n    return {\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'), // 当前用户信息\r\n      appointments: [], // 挂号信息列表\r\n      loading: false, // 加载状态\r\n      error: '', // 错误信息\r\n      currentPage: 1, // 当前页码\r\n      pageSize: 10, // 每页显示条数\r\n      totalAppointments: 0 // 总条数\r\n    };\r\n  },\r\n  created() {\r\n    this.fetchTodayAppointments(1); // 初始化加载今日预约信息\r\n  },\r\n  methods: {\r\n    // 获取今日预约信息\r\n    fetchTodayAppointments(pageNumber) {\r\n      this.loading = true;\r\n      this.error = '';\r\n      this.currentPage = pageNumber;\r\n\r\n      this.$request.get('/doctor/selectAppointments', {\r\n        params: {\r\n          pageNum: this.currentPage,\r\n          pageSize: this.pageSize,\r\n          userId: this.user.id // 传递当前用户 ID\r\n        }\r\n      })\r\n          .then(response => {\r\n            if (response.data && response.data.list) {\r\n              this.appointments = response.data.list;\r\n            }\r\n            if (response.data && response.data.total) {\r\n              this.totalAppointments = response.data.total;\r\n            }\r\n          })\r\n          .catch(error => {\r\n            console.error('获取今日预约信息失败:', error);\r\n            this.error = '获取今日预约信息失败，请稍后重试';\r\n          })\r\n          .finally(() => {\r\n            this.loading = false;\r\n          });\r\n    },\r\n\r\n    // 分页切换\r\n    handlePageChange(newPage) {\r\n      this.fetchTodayAppointments(newPage);\r\n    },\r\n\r\n    // 查看挂号详情\r\n    handleViewDetails(row) {\r\n      this.$router.push({\r\n        name: 'RegistrationDetail',\r\n        params: { id: row.id } // 传递挂号 ID\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.today-appointments {\r\n  padding: 20px;\r\n}\r\n\r\n.loading,\r\n.error,\r\n.no-data {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n  font-size: 16px;\r\n  color: #666;\r\n}\r\n\r\n.error {\r\n  color: #f56c6c;\r\n}\r\n</style>"],"mappings":";AAsCA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MAAA;MACAC,YAAA;MAAA;MACAC,OAAA;MAAA;MACAC,KAAA;MAAA;MACAC,WAAA;MAAA;MACAC,QAAA;MAAA;MACAC,iBAAA;IACA;EACA;;EACAC,QAAA;IACA,KAAAC,sBAAA;EACA;;EACAC,OAAA;IACA;IACAD,uBAAAE,UAAA;MACA,KAAAR,OAAA;MACA,KAAAC,KAAA;MACA,KAAAC,WAAA,GAAAM,UAAA;MAEA,KAAAC,QAAA,CAAAC,GAAA;QACAC,MAAA;UACAC,OAAA,OAAAV,WAAA;UACAC,QAAA,OAAAA,QAAA;UACAU,MAAA,OAAAnB,IAAA,CAAAoB,EAAA;QACA;MACA,GACAC,IAAA,CAAAC,QAAA;QACA,IAAAA,QAAA,CAAAvB,IAAA,IAAAuB,QAAA,CAAAvB,IAAA,CAAAwB,IAAA;UACA,KAAAlB,YAAA,GAA