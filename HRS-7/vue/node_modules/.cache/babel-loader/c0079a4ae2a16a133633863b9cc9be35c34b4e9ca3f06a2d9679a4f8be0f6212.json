{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'PrescribingAdministration',\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      // 当前用户信息\n      prescribes: [],\n      loading: false,\n      // 加载状态\n      error: '',\n      // 错误信息\n      currentPage: 1,\n      // 当前页码\n      pageSize: 10,\n      // 每页显示条数\n      totalPrescribes: 0 // 总条数\n    };\n  },\n\n  created() {\n    this.fetchPrescribingAdministration(1); // 初始化加载今日预约信息\n  },\n\n  methods: {\n    // 获取今日预约信息\n    fetchPrescribingAdministration(pageNumber) {\n      this.loading = true;\n      this.error = '';\n      this.currentPage = pageNumber;\n      this.$request.get('/prescribe/selectPrescribe', {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          userId: this.user.id // 传递当前用户 ID\n        }\n      }).then(response => {\n        if (response.data && response.data.list) {\n          this.prescribes = response.data.list;\n        }\n        if (response.data && response.data.total) {\n          this.totalPrescribes = response.data.total;\n        }\n      }).catch(error => {\n        console.error('获取开药信息失败:', error);\n        this.error = '获取开药失败，请稍后重试';\n      }).finally(() => {\n        this.loading = false;\n      });\n    },\n    // 分页切换\n    handlePageChange(newPage) {\n      this.fetchTodayAppointments(newPage);\n    },\n    // 查看开药详情\n    handleViewDetails() {\n      this.$router.push({\n        name: 'PrescribeDetail'\n      });\n    },\n    // 格式化手机号（脱敏显示）\n    formatPhone(phone) {\n      if (phone && phone.length === 11) {\n        return phone.substring(0, 3) + '****' + phone.substring(7);\n      }\n      return phone;\n    }\n  }\n};","map":{"version":3,"names":["name","data","user","JSON","parse","localStorage","getItem","prescribes","loading","error","currentPage","pageSize","totalPrescribes","created","fetchPrescribingAdministration","methods","pageNumber","$request","get","params","pageNum","userId","id","then","response","list","total","catch","console","finally","handlePageChange","newPage","fetchTodayAppointments","handleViewDetails","$router","push","formatPhone","phone","length","substring"],"sources":["src/views/doctor/PrescribingAdministration.vue"],"sourcesContent":["<template>\r\n  <div class=\"today-appointments\">\r\n    <h2>开药管理</h2>\r\n\r\n    <!-- 加载状态 -->\r\n    <div v-if=\"loading\" class=\"loading\">加载中...</div>\r\n\r\n    <!-- 错误提示 -->\r\n    <div v-if=\"error\" class=\"error\">{{ error }}</div>\r\n\r\n    <!-- 挂号信息表格 -->\r\n    <el-table v-if=\"prescribes.length > 0\" :data=\"prescribes\" stripe>\r\n      <!-- 患者姓名 -->\r\n      <el-table-column prop=\"userName\" label=\"患者姓名\" width=\"120\"></el-table-column>\r\n\r\n      <!-- 患者性别 -->\r\n      <el-table-column prop=\"userGender\" label=\"性别\" width=\"80\"></el-table-column>\r\n\r\n      <!-- 手机号（脱敏显示） -->\r\n      <el-table-column label=\"手机号\" width=\"140\">\r\n        <template slot-scope=\"scope\">\r\n          {{ formatPhone(scope.row.userPhone) }}\r\n        </template>\r\n      </el-table-column>\r\n\r\n      <!-- 预约时间 -->\r\n      <el-table-column prop=\"date\" label=\"开药时间\" width=\"180\"></el-table-column>\r\n\r\n      <!-- 操作 -->\r\n      <!-- 操作 -->\r\n      <el-table-column label=\"操作\" width=\"200\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"primary\" size=\"small\" @click=\"handleViewDetails()\">查看详情</el-button>\r\n\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!-- 无数据提示 -->\r\n    <div v-else class=\"no-data\">无开药信息</div>\r\n\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n        background\r\n        layout=\"prev, pager, next\"\r\n        :current-page=\"currentPage\"\r\n        :page-size=\"pageSize\"\r\n        :total=\"totalPrescribes\"\r\n        @current-change=\"handlePageChange\"\r\n    ></el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'PrescribingAdministration',\r\n  data() {\r\n    return {\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'), // 当前用户信息\r\n      prescribes: [],\r\n      loading: false, // 加载状态\r\n      error: '', // 错误信息\r\n      currentPage: 1, // 当前页码\r\n      pageSize: 10, // 每页显示条数\r\n      totalPrescribes: 0 // 总条数\r\n    };\r\n  },\r\n  created() {\r\n    this.fetchPrescribingAdministration(1); // 初始化加载今日预约信息\r\n  },\r\n  methods: {\r\n    // 获取今日预约信息\r\n    fetchPrescribingAdministration(pageNumber) {\r\n      this.loading = true;\r\n      this.error = '';\r\n      this.currentPage = pageNumber;\r\n\r\n      this.$request.get('/prescribe/selectPrescribe', {\r\n        params: {\r\n          pageNum: this.currentPage,\r\n          pageSize: this.pageSize,\r\n          userId: this.user.id // 传递当前用户 ID\r\n        }\r\n      })\r\n          .then(response => {\r\n            if (response.data && response.data.list) {\r\n              this.prescribes = response.data.list;\r\n            }\r\n            if (response.data && response.data.total) {\r\n              this.totalPrescribes = response.data.total;\r\n            }\r\n          })\r\n          .catch(error => {\r\n            console.error('获取开药信息失败:', error);\r\n            this.error = '获取开药失败，请稍后重试';\r\n          })\r\n          .finally(() => {\r\n            this.loading = false;\r\n          });\r\n    },\r\n\r\n    // 分页切换\r\n    handlePageChange(newPage) {\r\n      this.fetchTodayAppointments(newPage);\r\n    },\r\n\r\n    // 查看开药详情\r\n    handleViewDetails() {\r\n      this.$router.push({\r\n        name: 'PrescribeDetail',\r\n      });\r\n    },\r\n\r\n\r\n    // 格式化手机号（脱敏显示）\r\n    formatPhone(phone) {\r\n      if (phone && phone.length === 11) {\r\n        return phone.substring(0, 3) + '****' + phone.substring(7);\r\n      }\r\n      return phone;\r\n    },\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.today-appointments {\r\n  padding: 20px;\r\n}\r\n\r\n.loading,\r\n.error,\r\n.no-data {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n  font-size: 16px;\r\n  color: #666;\r\n}\r\n\r\n.error {\r\n  color: #f56c6c;\r\n}\r\n\r\n.completed-text {\r\n  display: inline-block;\r\n  padding: 4px 8px;\r\n  background-color: #e8f5e9; /* 浅绿色背景 */\r\n  color: #4caf50; /* 绿色文字 */\r\n  border-radius: 4px; /* 圆角 */\r\n  font-size: 12px; /* 调整字体大小 */\r\n  font-weight: bold; /* 加粗字体 */\r\n  margin-left: 10px\r\n}\r\n</style>"],"mappings":";AAsDA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MAAA;MACAC,UAAA;MACAC,OAAA;MAAA;MACAC,KAAA;MAAA;MACAC,WAAA;MAAA;MACAC,QAAA;MAAA;MACAC,eAAA;IACA;EACA;;EACAC,QAAA;IACA,KAAAC,8BAAA;EACA;;EACAC,OAAA;IACA;IACAD,+BAAAE,UAAA;MACA,KAAAR,OAAA;MACA,KAAAC,KAAA;MACA,KAAAC,WAAA,GAAAM,UAAA;MAEA,KAAAC,QAAA,CAAAC,GAAA;QACAC,MAAA;UACAC,OAAA,OAAAV,WAAA;UACAC,QAAA,OAAAA,QAAA;UACAU,MAAA,OAAAnB,IAAA,CAAAoB,EAAA;QACA;MACA,GACAC,IAAA,CAAAC,QAAA;QACA,IAAAA,QAAA,CAAAvB,IAAA,IAAAuB,QAAA,CAAAvB,IAAA,CAAAwB,IAAA;UACA,KAAAlB,UAAA,GAAAiB,QAAA,CAAAvB,IAAA,CAAAwB,IAAA;QACA;QACA,IAAAD,QAAA,CAAAvB,IAAA,IAAAuB,QAAA,CAAAvB,IAAA,CAAAyB,KAAA;UACA,KAAAd,eAAA,GAAAY,QAAA,CAAAvB,IAAA,CAAAyB,KAAA;QACA;MACA,GACAC,KAAA,CAAAlB,KAAA;QACAmB,OAAA,CAAAnB,KAAA,cAAAA,KAAA;QACA,KAAAA,KAAA;MACA,GACAoB,OAAA;QACA,KAAArB,OAAA;MACA;IACA;IAEA;IACAsB,iBAAAC,OAAA;MACA,KAAAC,sBAAA,CAAAD,OAAA;IACA;IAEA;IACAE,kBAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;QACAnC,IAAA;MACA;IACA;IAGA;IACAoC,YAAAC,KAAA;MACA,IAAAA,KAAA,IAAAA,KAAA,CAAAC,MAAA;QACA,OAAAD,KAAA,CAAAE,SAAA,kBAAAF,KAAA,CAAAE,SAAA;MACA;MACA,OAAAF,KAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}