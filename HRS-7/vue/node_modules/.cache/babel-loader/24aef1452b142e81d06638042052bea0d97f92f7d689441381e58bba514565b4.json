{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'Reservation',\n  data() {\n    return {\n      searchKeyword: '',\n      // 搜索关键词\n      hospitalList: [],\n      // 医院列表\n      dialogVisible: false,\n      // 模态框是否显示\n      selectedHospital: {},\n      // 当前选中的医院信息\n      tableData: [],\n      // 所有的数据\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 10,\n      // 每页显示的个数\n      total: 0,\n      title: null,\n      name: null,\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      ids: []\n    };\n  },\n  created() {\n    this.getHospitalList(1);\n  },\n  methods: {\n    getHospitalList(pageNum) {\n      // 分页查询\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/hospital/selectPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          name: this.name\n        }\n      }).then(res => {\n        this.tableData = res.data?.list;\n        this.total = res.data?.total;\n      });\n    },\n    reset() {\n      this.name = null;\n      this.load(1);\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum);\n    },\n    showHospitalDetail(row) {\n      this.selectedHospital = row;\n      this.dialogVisible = true;\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","searchKeyword","hospitalList","dialogVisible","selectedHospital","tableData","pageNum","pageSize","total","title","user","JSON","parse","localStorage","getItem","ids","created","getHospitalList","methods","$request","get","params","then","res","list","reset","load","handleCurrentChange","showHospitalDetail","row"],"sources":["src/views/manager/Reservation.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- 搜索框 -->\r\n    <div class=\"search-container\">\r\n      <el-input v-model=\"searchKeyword\" placeholder=\"请输入医院名称\" @keyup.enter=\"getHospitalList(1)\"></el-input>\r\n      <el-button @click=\"getHospitalList(1)\">搜索</el-button>\r\n    </div>\r\n\r\n    <!-- 医院列表 -->\r\n    <el-table :data=\"hospitalList\" stripe @row-click=\"showHospitalDetail\">\r\n      <el-table-column prop=\"name\" label=\"医院名称\"></el-table-column>\r\n      <!-- 可以根据实际情况添加更多列 -->\r\n    </el-table>\r\n\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n        background\r\n        layout=\"prev, pager, next\"\r\n        :current-page=\"pageNum\"\r\n        :page-size=\"pageSize\"\r\n        :total=\"total\"\r\n        @current-change=\"handleCurrentChange\"\r\n    ></el-pagination>\r\n\r\n    <!-- 医院详情模态框 -->\r\n    <el-dialog :visible.sync=\"dialogVisible\" title=\"医院详情\">\r\n      <template #content>\r\n        <p><strong>医院名称:</strong> {{ selectedHospital.name }}</p>\r\n        <p><strong>医院地址:</strong> {{ selectedHospital.address }}</p>\r\n        <p><strong>联系电话:</strong> {{ selectedHospital.phone }}</p>\r\n        <p><strong>医院等级:</strong> {{ selectedHospital.level }}</p>\r\n        <p><strong>医院简介:</strong> {{ selectedHospital.introduction }}</p>\r\n        <p><strong>科室列表:</strong></p>\r\n        <ul>\r\n          <li v-for=\"(department, index) in selectedHospital.departments\" :key=\"index\">\r\n            {{ department }}\r\n          </li>\r\n        </ul>\r\n      </template>\r\n      <template #footer>\r\n        <el-button @click=\"dialogVisible = false\">关闭</el-button>\r\n      </template>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'Reservation',\r\n  data() {\r\n    return {\r\n      searchKeyword: '', // 搜索关键词\r\n      hospitalList: [], // 医院列表\r\n      dialogVisible: false, // 模态框是否显示\r\n      selectedHospital: {} ,// 当前选中的医院信息\r\n      tableData: [],  // 所有的数据\r\n      pageNum: 1,   // 当前的页码\r\n      pageSize: 10,  // 每页显示的个数\r\n      total: 0,\r\n      title: null,\r\n      name:null,\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      ids: []\r\n    };\r\n  },\r\n  created() {\r\n    this.getHospitalList(1);\r\n  },\r\n  methods: {\r\n    getHospitalList(pageNum) {  // 分页查询\r\n      if (pageNum) this.pageNum = pageNum\r\n      this.$request.get('/hospital/selectPage', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          name: this.name,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data?.list\r\n        this.total = res.data?.total\r\n      })\r\n    },\r\n    reset() {\r\n      this.name = null\r\n      this.load(1)\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.load(pageNum)\r\n    },\r\n    showHospitalDetail(row) {\r\n      this.selectedHospital = row;\r\n      this.dialogVisible = true;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.search-container {\r\n  margin-bottom: 20px;\r\n}\r\n</style>"],"mappings":"AA+CA,OAAAA,KAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,aAAA;MAAA;MACAC,YAAA;MAAA;MACAC,aAAA;MAAA;MACAC,gBAAA;MAAA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAC,KAAA;MACAV,IAAA;MACAW,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,GAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,eAAA;EACA;EACAC,OAAA;IACAD,gBAAAX,OAAA;MAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAa,QAAA,CAAAC,GAAA;QACAC,MAAA;UACAf,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAR,IAAA,OAAAA;QACA;MACA,GAAAuB,IAAA,CAAAC,GAAA;QACA,KAAAlB,SAAA,GAAAkB,GAAA,CAAAvB,IAAA,EAAAwB,IAAA;QACA,KAAAhB,KAAA,GAAAe,GAAA,CAAAvB,IAAA,EAAAQ,KAAA;MACA;IACA;IACAiB,MAAA;MACA,KAAA1B,IAAA;MACA,KAAA2B,IAAA;IACA;IACAC,oBAAArB,OAAA;MACA,KAAAoB,IAAA,CAAApB,OAAA;IACA;IACAsB,mBAAAC,GAAA;MACA,KAAAzB,gBAAA,GAAAyB,GAAA;MACA,KAAA1B,aAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}