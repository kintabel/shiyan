option label=\"医生 A\" value=\"doctor-a\"></el-option>\r\n              <el-option label=\"医生 B\" value=\"doctor-b\"></el-option>\r\n              <el-option label=\"医生 C\" value=\"doctor-c\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item label=\"选择日期\">\r\n            <el-date-picker\r\n                v-model=\"form.date\"\r\n                type=\"date\"\r\n                placeholder=\"选择日期\"\r\n            ></el-date-picker>\r\n          </el-form-item>\r\n          <el-form-item label=\"选择时段\">\r\n            <el-select v-model=\"form.timeSlot\" placeholder=\"请选择时段\">\r\n              <el-option label=\"上午\" value=\"morning\"></el-option>\r\n              <el-option label=\"下午\" value=\"afternoon\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item>\r\n            <el-button type=\"primary\" @click=\"submitForm\">提交挂号</el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </div>\r\n\r\n      <!-- 科室分类 -->\r\n      <div class=\"department-list\">\r\n        <h2>科室分类</h2>\r\n        <ul>\r\n          <li v-for=\"(department, index) in departments\" :key=\"index\">\r\n            {{ department }}\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 底部信息 -->\r\n    <div class=\"hospital-register-footer\">\r\n      <p>联系电话：010 - 12345678</p>\r\n      <p>地址：北京市 XX 区 XX 路 XX 号</p>\r\n      <p>版权所有 &copy; 2025 医院挂号系统</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"HospitalRegister\",\r\n  data() {\r\n    return {\r\n      form: {\r\n        department: \"\",\r\n        doctor: \"\",\r\n        date: \"\",\r\n        timeSlot: \"\"\r\n      },\r\n      departments: [\"内科\", \"外科\", \"妇产科\", \"儿科\", \"眼科\"]\r\n    };\r\n  },\r\n  methods: {\r\n    submitForm() {\r\n      // 这里可以添加提交表单的逻辑，如发送请求到后端\r\n      console.log(\"提交的挂号信息：\", this.form);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n@import \"@/assets/css/hospital.css\";\r\n\r\n</style>"],"mappings":"AA6EA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA;QACAC,UAAA;QACAC,MAAA;QACAC,IAAA;QACAC,QAAA;MACA;MACAC,WAAA;IACA;EACA;EACAC,OAAA;IACAC,WAAA;MACA;MACAC,OAAA,CAAAC,GAAA,kBAAAT,IAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}