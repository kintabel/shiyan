{"ast":null,"code":"export default {\n  name: \"Login\",\n  data() {\n    return {\n      form: {},\n      rules: {\n        account: [{\n          required: true,\n          message: '请输入账号',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }]\n      },\n      imgs: [\n      //图形滑块验证的图片\n      require('@/assets/imgs/hk1.jpg'), require('@/assets/imgs/hk2.jpg'), require('@/assets/imgs/hk3.jpg')],\n      slideVerifyShow: false //默认滑块验证为不显示\n    };\n  },\n\n  created() {},\n  methods: {\n    onSuccess() {\n      this.$request.post('/login', this.form).then(res => {\n        if (res.code === '200') {\n          localStorage.setItem(\"xm-user\", JSON.stringify(res.data)); // 存储用户数据\n          this.$message.success('登录成功');\n          setTimeout(() => {\n            if (res.data.role === \"ADMIN\") {\n              //this.$router.push('/')  // 跳转后台\n              console.log('跳转后台');\n              console.log(res.data.token);\n              location.href = '/topAdmin';\n            } else if (res.data.role === \"HospitalADMIN\") {\n              location.href = '/hospitalAdmin'; //本地跳转\n            } else if (res.data.role === \"USER\") {\n              //this.$router.push(('/front/home'))//路由跳转主页\n              location.href = '/home'; //本地跳转\n            } else if (res.data.role === \"DOCTOR\") {\n              console.log(res.data.token);\n              //location.href = '/doctor/home'\n              location.href = '/doctor';\n            }\n          });\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    login() {\n      this.$refs['formRef'].validate(valid => {\n        if (valid) {\n          // 验证通过\n          this.slideVerifyShow = true; //滑块验证显示\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","form","rules","account","required","message","trigger","password","imgs","require","slideVerifyShow","created","methods","onSuccess","$request","post","then","res","code","localStorage","setItem","JSON","stringify","$message","success","setTimeout","role","console","log","token","location","href","error","msg","login","$refs","validate","valid"],"sources":["E:\\专业设计项目\\HRS\\HRS-7\\vue\\src\\views\\Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div style=\"height: 60px; position: fixed; top: 0; display: flex;justify-items: center;padding-left: 20px\">\r\n      <img src=\"@/assets/imgs/logo.png\" alt=\"\" style=\"width: 50px\">\r\n      <span style=\"color: #409EFF; font-size: 24px;font-weight: bold;margin-left: 5px\">医院挂号系统</span>\r\n    </div>\r\n    <div style=\"flex: 1;display: flex;justify-content: center\">\r\n      <img src=\"@/assets/imgs/bg.png\" alt=\"\" style=\"width: 80%\">\r\n    </div>\r\n\r\n    <div style=\"flex: 1;display: flex;justify-content: center\">\r\n      <div\r\n          style=\"width: 350px; position: relative; padding: 50px 30px; box-shadow: 0 0 10px rgba(0,0,0,.2); background-color: white; border-radius: 5px;\">\r\n        <div style=\"position: absolute;background-color:white; top:100px;z-index:999\" v-if=\"slideVerifyShow\">\r\n          <slide-verify :l=\"42\"\r\n                        :r=\"10\"\r\n                        :w=\"300\"\r\n                        :h=\"155\"\r\n                        :accuracy=\"5\"\r\n                        :imgs=\"imgs\"\r\n                        slider-text=\"向右滑动\"\r\n                        @success=\"onSuccess\"\r\n                        @fail=\"onFail\"\r\n                        @refresh=\"onRefresh\"\r\n                        ref=\"slideVerifyRef\"\r\n          ></slide-verify>\r\n        </div>\r\n        <div style=\"text-align: center; font-size: 24px; font-weight: bold; margin-bottom: 30px; color: #333\">欢 迎 登\r\n          录\r\n        </div>\r\n        <el-form :model=\"form\" :rules=\"rules\" ref=\"formRef\">\r\n          <el-form-item prop=\"account\">\r\n            <el-input size=\"medium\" prefix-icon=\"el-icon-user\" placeholder=\"请输入账号\"\r\n                      v-model=\"form.account\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item prop=\"password\">\r\n            <el-input size=\"medium\" prefix-icon=\"el-icon-lock\" placeholder=\"请输入密码\" show-password\r\n                      v-model=\"form.password\"></el-input>\r\n          </el-form-item>\r\n\r\n          <el-form-item>\r\n            <el-button size=\"medium\" style=\"width: 100%; background-color: #2a60c9; border-color: #2a60c9; color: white\"\r\n                       @click=\"login\">登 录\r\n            </el-button>\r\n          </el-form-item>\r\n          <div style=\"display: flex; align-items: center\">\r\n            <div style=\"flex: 1\"></div>\r\n            <div style=\"flex: 1; text-align: right\">\r\n              还没有账号？请 <a href=\"/register\">注册</a>\r\n            </div>\r\n          </div>\r\n        </el-form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Login\",\r\n  data() {\r\n    return {\r\n      form: {},\r\n      rules: {\r\n        account: [\r\n          {required: true, message: '请输入账号', trigger: 'blur'},\r\n        ],\r\n        password: [\r\n          {required: true, message: '请输入密码', trigger: 'blur'},\r\n        ]\r\n      },\r\n      imgs:[ //图形滑块验证的图片\r\n        require('@/assets/imgs/hk1.jpg'),\r\n        require('@/assets/imgs/hk2.jpg'),\r\n        require('@/assets/imgs/hk3.jpg')\r\n      ],\r\n      slideVerifyShow:false  //默认滑块验证为不显示\r\n    }\r\n  },\r\n  created() {\r\n\r\n  },\r\n  methods: {\r\n    onSuccess(){\r\n      this.$request.post('/login', this.form).then(res => {\r\n        if (res.code === '200') {\r\n          localStorage.setItem(\"xm-user\", JSON.stringify(res.data))  // 存储用户数据\r\n          this.$message.success('登录成功')\r\n\r\n          setTimeout(() => {\r\n            if (res.data.role === \"ADMIN\") {\r\n              //this.$router.push('/')  // 跳转后台\r\n              console.log('跳转后台')\r\n              console.log(res.data.token)\r\n              location.href = '/topAdmin'\r\n            }\r\n            else if(res.data.role === \"HospitalADMIN\"){\r\n              location.href = '/hospitalAdmin' //本地跳转\r\n            }else if(res.data.role === \"USER\"){\r\n              //this.$router.push(('/front/home'))//路由跳转主页\r\n              location.href = '/home' //本地跳转\r\n            }else if(res.data.role === \"DOCTOR\"){\r\n              console.log(res.data.token)\r\n              //location.href = '/doctor/home'\r\n              location.href = '/doctor'\r\n            }\r\n          })\r\n\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n\r\n      })\r\n    },\r\n    login() {\r\n      this.$refs['formRef'].validate((valid) => {\r\n        if (valid) {\r\n          // 验证通过\r\n          this.slideVerifyShow = true;  //滑块验证显示\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  height: 100vh;\r\n  overflow: hidden;\r\n//background-image: url(\"@/assets/imgs/bg.jpg\"); background-size: 100%;\r\n  display: flex;\r\n  align-items: center;\r\n//justify-content: center; //color: #666;\r\n}\r\n\r\na {\r\n  color: #2a60c9;\r\n}\r\n</style>"],"mappings":"AA0DA,eAAe;EACbA,IAAI,EAAE,OAAO;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,CAAC,CAAC;MACRC,KAAK,EAAE;QACLC,OAAO,EAAE,CACP;UAACC,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,OAAO;UAAEC,OAAO,EAAE;QAAM,CAAC,CACpD;QACDC,QAAQ,EAAE,CACR;UAACH,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,OAAO;UAAEC,OAAO,EAAE;QAAM,CAAC;MAEvD,CAAC;MACDE,IAAI,EAAC;MAAE;MACLC,OAAO,CAAC,uBAAuB,CAAC,EAChCA,OAAO,CAAC,uBAAuB,CAAC,EAChCA,OAAO,CAAC,uBAAuB,EAChC;MACDC,eAAe,EAAC,KAAI,CAAG;IACzB;EACF,CAAC;;EACDC,OAAOA,CAAA,EAAG,CAEV,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAAA,EAAE;MACT,IAAI,CAACC,QAAQ,CAACC,IAAI,CAAC,QAAQ,EAAE,IAAI,CAACd,IAAI,CAAC,CAACe,IAAI,CAACC,GAAE,IAAK;QAClD,IAAIA,GAAG,CAACC,IAAG,KAAM,