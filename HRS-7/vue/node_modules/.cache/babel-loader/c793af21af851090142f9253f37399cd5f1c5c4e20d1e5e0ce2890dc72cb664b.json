{"ast":null,"code":"export default {\n  name: 'OrderDetail',\n  data() {\n    return {\n      registrationDetail: null,\n      isLoading: true\n    };\n  },\n  created() {\n    this.fetchOrderDetail();\n  },\n  methods: {\n    fetchOrderDetail() {\n      const orderId = Number(this.$route.query.order);\n      if (!orderId) {\n        this.handleError('订单号参数缺失', null);\n        this.isLoading = false;\n        return;\n      }\n      this.$request.get('/registration/detail', {\n        params: {\n          orderId: orderId\n        }\n      }).then(response => {\n        if (response.data) {\n          this.registrationDetail = response.data;\n        } else {\n          this.handleError('获取订单详情失败', response.data);\n        }\n      }).catch(error => {\n        this.handleError('获取订单详情失败', error);\n      }).finally(() => {\n        this.isLoading = false;\n      });\n    },\n    handleError(message, error) {\n      console.error(message, error);\n      this.$message.error(`${message}，请稍后重试`);\n    }\n  }\n};","map":{"version":3,"names":["name","data","registrationDetail","isLoading","created","fetchOrderDetail","methods","orderId","Number","$route","query","order","handleError","$request","get","params","then","response","catch","error","finally","message","console","$message"],"sources":["src/views/user/RegistrationDetail.vue"],"sourcesContent":["<template>\r\n  <div class=\"order-detail-page\">\r\n    <!-- 数据加载中提示 -->\r\n    <div v-if=\"isLoading\" class=\"loading-container\">\r\n      <el-spinner />\r\n      <span>正在加载订单详情，请稍候...</span>\r\n    </div>\r\n    <div v-else>\r\n      <h2>订单详情</h2>\r\n      <el-form label-width=\"120px\" v-if=\"registrationDetail\">\r\n        <el-form-item label=\"订单编号\">\r\n          <span>{{ registrationDetail.order }}</span>\r\n        </el-form-item>\r\n        <el-form-item label=\"医院\">\r\n          <span>{{ registrationDetail.hospitalName }}</span>\r\n        </el-form-item>\r\n        <el-form-item label=\"科室\">\r\n          <span>{{ registrationDetail.departmentName }}</span>\r\n        </el-form-item>\r\n        <el-form-item label=\"医生姓名\">\r\n          <span>{{ registrationDetail.doctorName }}</span>\r\n        </el-form-item>\r\n        <el-form-item label=\"挂号时间\">\r\n          <span>{{ registrationDetail.registerTime }}</span>\r\n        </el-form-item>\r\n        <el-form-item label=\"挂号班次\">\r\n          <span>{{ registrationDetail.shiftType === 'DAY' ? '白班' : '夜班' }}</span>\r\n        </el-form-item>\r\n        <el-form-item label=\"订单价格\">\r\n          <span>{{ registrationDetail.price }} 元</span>\r\n        </el-form-item>\r\n        <el-form-item label=\"用户姓名\">\r\n          <span>{{ registrationDetail.userName }}</span>\r\n        </el-form-item>\r\n        <el-form-item label=\"用户性别\">\r\n          <span>{{ registrationDetail.userGender }}</span>\r\n        </el-form-item>\r\n        <el-form-item label=\"用户联系方式\">\r\n          <span>{{ registrationDetail.userPhone }}</span>\r\n        </el-form-item>\r\n        <el-form-item label=\"订单提交日期\">\r\n          <span>{{ registrationDetail.orderTime }}</span>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div v-else>\r\n        <p>获取订单详情失败，请稍后重试。</p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'OrderDetail',\r\n  data() {\r\n    return {\r\n      registrationDetail: null,\r\n      isLoading: true\r\n    };\r\n  },\r\n  created() {\r\n    this.fetchOrderDetail();\r\n  },\r\n  methods: {\r\n    fetchOrderDetail() {\r\n      const orderId = Number(this.$route.query.order);\r\n      if (!orderId) {\r\n        this.handleError('订单号参数缺失', null);\r\n        this.isLoading = false;\r\n        return;\r\n      }\r\n      this.$request.get('/registration/detail', {\r\n        params: {\r\n          orderId: orderId\r\n        }\r\n      })\r\n          .then(response => {\r\n            if (response.data) {\r\n              this.registrationDetail = response.data;\r\n   