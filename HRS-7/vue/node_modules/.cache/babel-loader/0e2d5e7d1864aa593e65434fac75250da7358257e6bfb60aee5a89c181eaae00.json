{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'SelectRegistration',\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      // 当前用户信息\n      registrations: [],\n      // 挂号信息列表\n      loading: false,\n      // 加载状态\n      error: '',\n      // 错误信息\n      currentPage: 1,\n      // 当前页码\n      pageSize: 10,\n      // 每页显示条数\n      totalRegistrations: 0 // 总条数\n    };\n  },\n\n  created() {\n    this.fetchRegistrations(1); // 初始化加载今日预约信息\n  },\n\n  methods: {\n    // 获取今日预约信息\n    fetchRegistrations(pageNumber) {\n      this.loading = true;\n      this.error = '';\n      this.currentPage = pageNumber;\n      this.$request.get('/user/selectRegistrations', {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          userId: this.user.id // 传递当前用户 ID\n        }\n      }).then(response => {\n        if (response.data && response.data.list) {\n          this.registrations = response.data.list;\n        }\n        if (response.data && response.data.total) {\n          this.totalRegistrations = response.data.total;\n        }\n      }).catch(error => {\n        console.error('获取用户挂号信息失败:', error);\n        this.error = '获取用户挂号信息失败，请稍后重试';\n      }).finally(() => {\n        this.loading = false;\n      });\n    },\n    // 分页切换\n    handlePageChange(newPage) {\n      this.fetchRegistrations(newPage); // 修改为调用正确的方法\n    },\n\n    // 查看挂号详情\n    handleViewDetails(row) {\n      console.log(row.order);\n      this.$router.push({\n        name: 'SelectRegistrationDetail',\n        params: {\n          id: row.order\n        } // 传递挂号 ID\n      });\n    },\n\n    // 格式化手机号（脱敏显示）\n    formatPhone(phone) {\n      if (phone && phone.length === 11) {\n        return phone.substring(0, 3) + '****' + phone.substring(7);\n      }\n      return phone;\n    }\n  }\n};","map":{"version":3,"names":["name","data","user","JSON","parse","localStorage","getItem","registrations","loading","error","currentPage","pageSize","totalRegistrations","created","fetchRegistrations","methods","pageNumber","$request","get","params","pageNum","userId","id","then","response","list","total","catch","console","finally","handlePageChange","newPage","handleViewDetails","row","log","order","$router","push","formatPhone","phone","length","substring"],"sources":["E:\\专业设计项目\\HRS\\HRS-7\\vue\\src\\views\\user\\SelectRegistration.vue"],"sourcesContent":["<template>\r\n  <div class=\"today-appointments\">\r\n    <h2>挂号信息</h2>\r\n\r\n    <!-- 加载状态 -->\r\n    <div v-if=\"loading\" class=\"loading\">加载中...</div>\r\n\r\n    <!-- 错误提示 -->\r\n    <div v-if=\"error\" class=\"error\">{{ error }}</div>\r\n\r\n    <!-- 挂号信息表格 -->\r\n    <el-table v-if=\"registrations.length > 0\" :data=\"registrations\" stripe>\r\n      <!-- 患者姓名 -->\r\n      <el-table-column prop=\"userName\" label=\"患者姓名\" width=\"120\"></el-table-column>\r\n\r\n      <!-- 患者性别 -->\r\n      <el-table-column prop=\"userGender\" label=\"性别\" width=\"80\"></el-table-column>\r\n\r\n      <!-- 手机号（脱敏显示） -->\r\n      <el-table-column label=\"手机号\" width=\"140\">\r\n        <template slot-scope=\"scope\">\r\n          {{ formatPhone(scope.row.userPhone) }}\r\n        </template>\r\n      </el-table-column>\r\n\r\n      <!-- 预约时间 -->\r\n      <el-table-column prop=\"registerTime\" label=\"预约时间\" width=\"180\"></el-table-column>\r\n\r\n      <!-- 操作 -->\r\n      <el-table-column label=\"操作\" width=\"200\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"primary\" size=\"small\" @click=\"handleViewDetails(scope.row)\">查看详情</el-button>\r\n\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!-- 无数据提示 -->\r\n    <div v-else class=\"no-data\">暂无挂号信息</div>\r\n\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n        background\r\n        layout=\"prev, pager, next\"\r\n        :current-page=\"currentPage\"\r\n        :page-size=\"pageSize\"\r\n        :total=\"totalRegistrations\"\r\n        @current-change=\"handlePageChange\"\r\n    ></el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'SelectRegistration',\r\n  data() {\r\n    return {\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'), // 当前用户信息\r\n      registrations: [], // 挂号信息列表\r\n      loading: false, // 加载状态\r\n      error: '', // 错误信息\r\n      currentPage: 1, // 当前页码\r\n      pageSize: 10, // 每页显示条数\r\n      totalRegistrations: 0 // 总条数\r\n    };\r\n  },\r\n  created() {\r\n    this.fetchRegistrations(1); // 初始化加载今日预约信息\r\n  },\r\n  methods: {\r\n    // 获取今日预约信息\r\n    fetchRegistrations(pageNumber) {\r\n      this.loading = true;\r\n      this.error = '';\r\n      this.currentPage = pageNumber;\r\n\r\n      this.$request.get('/user/selectRegistrations', {\r\n        params: {\r\n          pageNum: this.currentPage,\r\n          pageSize: this.pageSize,\r\n          userId: this.user.id // 传递当前用户 ID\r\n        }\r\n      })\r\n          .then(response => {\r\n            if (response.data && response.data.list) {\r\n              this.registrations = response.data.list;\r\n            }\r\n            if (response.data && response.data.total) {\r\n              this.totalRegistrations = response.data.total;\r\n            }\r\n          })\r\n          .catch(error => {\r\n            console.error('获取用户挂号信息失败:', error);\r\n            this.error = '获取用户挂号信息失败，请稍后重试';\r\n          })\r\n          .finally(() => {\r\n            this.loading = false;\r\n          });\r\n    },\r\n\r\n    // 分页切换\r\n    handlePageChange(newPage) {\r\n      this.fetchRegistrations(newPage); // 修改为调用正确的方法\r\n    },\r\n\r\n    // 查看挂号详情\r\n    handleViewDetails(row) {\r\n      console.log(row.order);\r\n      this.$router.push({\r\n        name: 'SelectRegistrationDetail',\r\n        params: {id: row.order} // 传递挂号 ID\r\n      });\r\n    },\r\n    // 格式化手机号（脱敏显示）\r\n    formatPhone(phone) {\r\n      if (phone && phone.length === 11) {\r\n        return phone.substring(0, 3) + '****' + phone.substring(7);\r\n      }\r\n      return phone;\r\n    },\r\n\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.today-appointments {\r\n  padding: 20px;\r\n}\r\n\r\n.loading,\r\n.error,\r\n.no-data {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n  font-size: 16px;\r\n  color: #666;\r\n}\r\n\r\n.error {\r\n  color: #f56c6c;\r\n}\r\n\r\n.completed-text {\r\n  display: inline-block;\r\n  padding: 4px 8px;\r\n  background-color: #e8f5e9; /* 浅绿色背景 */\r\n  color: #4caf50; /* 绿色文字 */\r\n  border-radius: 4px; /* 圆角 */\r\n  font-size: 12px; /* 调整字体大小 */\r\n  font-weight: bold; /* 加粗字体 */\r\n  margin-left: 10px\r\n}\r\n</style>"],"mappings":";AAqDA,eAAe;EACbA,IAAI,EAAE,oBAAoB;EAC1BC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAEC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,SAAS,KAAK,IAAI,CAAC;MAAE;MAC3DC,aAAa,EAAE,EAAE;MAAE;MACnBC,OAAO,EAAE,KAAK;MAAE;MAChBC,KAAK,EAAE,EAAE;MAAE;MACXC,WAAW,EAAE,CAAC;MAAE;MAChBC,QAAQ,EAAE,EAAE;MAAE;MACdC,kBAAkB,EAAE,EAAE;IACxB,CAAC;EACH,CAAC;;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,kBAAkB,CAAC,CAAC,CAAC,EAAE;EAC9B,CAAC;;EACDC,OAAO,EAAE;IACP;IACAD,kBAAkBA,CAACE,UAAU,EAAE;MAC7B,IAAI,CAACR,OAAM,GAAI,IAAI;MACnB,IAAI,CAACC,KAAI,GAAI,EAAE;MACf,IAAI,CAACC,WAAU,GAAIM,UAAU;MAE7B,IAAI,CAACC,QAAQ,CAACC,GAAG,CAAC,2BAA2B,EAAE;QAC7CC,MAAM,EAAE;UACNC,OAAO,EAAE,IAAI,CAACV,WAAW;UACzBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBU,MAAM,EAAE,IAAI,CAACnB,IAAI,CAACoB,EAAC,CAAE;QACvB;MACF,CAAC,EACIC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIA,QAAQ,CAACvB,IAAG,IAAKuB,QAAQ,CAACvB,IAAI,CAACwB,IAAI,EAAE;UACvC,IAAI,CAAClB,aAAY,GAAIiB,QAAQ,CAACvB,IAAI,CAACwB,IAAI;QACzC;QACA,IAAID,QAAQ,CAACvB,IAAG,IAAKuB,QAAQ,CAACvB,IAAI,CAACyB,KAAK,EAAE;UACxC,IAAI,CAACd,kBAAiB,GAAIY,QAAQ,CAACvB,IAAI,CAACyB,KAAK;QAC/C;MACF,CAAC,EACAC,KAAK,CAAClB,KAAI,IAAK;QACdmB,OAAO,CAACnB,KAAK,CAAC,aAAa,EAAEA,KAAK,CAAC;QACnC,IAAI,CAACA,KAAI,GAAI,kBAAkB;MACjC,CAAC,EACAoB,OAAO,CAAC,MAAM;QACb,IAAI,CAACrB,OAAM,GAAI,KAAK;MACtB,CAAC,CAAC;IACR,CAAC;IAED;IACAsB,gBAAgBA,C