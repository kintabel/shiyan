{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"HospitalManager\",\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}')\n    };\n  },\n  created() {\n    if (!this.user.id) {\n      this.$router.push('/login');\n    }\n  },\n  methods: {\n    updateUser() {\n      this.user = JSON.parse(localStorage.getItem('xm-user') || '{}'); // 重新获取下用户的最新信息\n    },\n\n    goToPerson() {\n      if (this.user.role === 'ADMIN') {\n        this.$router.push('/adminPerson');\n      } else {\n        this.$router.push('/person'); // 普通用户跳转到个人信息页面\n      }\n    },\n\n    logout() {\n      localStorage.removeItem('xm-user'); // 清除用户信息\n      console.log('用户已退出登录'); // 调试日志\n      this.$router.push(\"/login\").catch(err => {\n        console.error('路由跳转失败:', err); // 捕获路由跳转错误\n      });\n    },\n\n    selectRegistration() {\n      this.$router.push(\"/selectRegistration\");\n    },\n    goToAppointmentRegistration(id) {\n      if (id = 1) {\n        this.$router.push({\n          path: '/Same-day-registration/reservation',\n          query: {\n            isAppointment: true\n          }\n        });\n      } else {\n        this.$router.push({\n          path: '/Same-day-registration/reservation',\n          query: {\n            isAppointment: false\n          }\n        });\n      }\n    },\n    goToArchieve() {\n      this.$router.push(\"/archives-management\");\n    },\n    goToWaitingQueue() {\n      this.$router.push(\"/Waiting-queues\");\n    }\n  }\n};","map":{"version":3,"names":["name","data","user","JSON","parse","localStorage","getItem","created","id","$router","push","methods","updateUser","goToPerson","role","logout","removeItem","console","log","catch","err","error","selectRegistration","goToAppointmentRegistration","path","query","isAppointment","goToArchieve","goToWaitingQueue"],"sources":["E:\\专业设计项目\\HRS\\HRS-7\\vue\\src\\views\\UserFront.vue"],"sourcesContent":["<template>\r\n  <div class=\"hospital-container\">\r\n    <!-- 顶部横幅 -->\r\n    <div class=\"hospital-header\">\r\n      <div class=\"hospital-header-left\">\r\n        <img src=\"@/assets/imgs/logo.png\" alt=\"医院 logo\"/>\r\n        <div class=\"title\">非著名的沧秋医院</div>\r\n      </div>\r\n      <div class=\"hospital-header-right\">\r\n        <!-- 根据用户是否登录显示不同内容 -->\r\n        <template v-if=\"user.id\">\r\n          <!-- 登录后显示头像和下拉菜单 -->\r\n          <el-dropdown>\r\n            <img :src=\"user.avatar\" alt=\"用户头像\" class=\"user-avatar\"/>\r\n            <template #dropdown>\r\n              <el-dropdown-menu>\r\n                <el-dropdown-item @click.native=\"goToPerson\">查看个人信息</el-dropdown-item>\r\n                <el-dropdown-item @click.native=\"logout\">退出登录</el-dropdown-item>\r\n                <el-dropdown-item @click.native=\"selectRegistration\">查看挂号信息</el-dropdown-item>\r\n              </el-dropdown-menu>\r\n            </template>\r\n          </el-dropdown>\r\n        </template>\r\n        <template v-else>\r\n          <!-- 未登录显示登录按钮 -->\r\n          <el-button type=\"text\" @click=\"$router.push('/login')\">登录</el-button>\r\n        </template>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 导航栏 -->\r\n    <div class=\"hospital-nav\">\r\n      <el-menu mode=\"horizontal\" :default-openeds=\"[]\" router style=\"border: none\" :default-active=\"$route.path\">\r\n        <!-- 首页 -->\r\n        <el-menu-item index=\"/home\">\r\n          <template #title>\r\n            <i class=\"el-icon-date\"></i>\r\n            <span>首页</span>\r\n          </template>\r\n        </el-menu-item>\r\n\r\n        <!-- 挂号 -->\r\n        <el-submenu index=\"register\">\r\n          <template #title>\r\n            <i class=\"el-icon-medal\"></i>\r\n            <span>挂号</span>\r\n          </template>\r\n          <el-menu-item @click=\"goToAppointmentRegistration(1)\">预约挂号</el-menu-item>\r\n          <el-menu-item index=\"/Same-day-registration/reservation\">当日挂号</el-menu-item>\r\n        </el-submenu>\r\n\r\n\r\n        <!-- 在线问诊 -->\r\n        <el-menu-item index=\"/userOnlineChat\">\r\n          <template #title>\r\n            <i class=\"el-icon-user-solid\"></i>\r\n            <span>在线问诊</span>\r\n          </template>\r\n        </el-menu-item>\r\n\r\n        <!-- 个人档案管理 -->\r\n        <el-menu-item   @click=\"goToArchieve()\">个人档案管理</el-menu-item>\r\n\r\n        <!-- 候诊队列查询 -->\r\n        <el-menu-item @click=\"goToWaitingQueue\">\r\n          <template #title>\r\n            <i class=\"el-icon-guide\"></i>\r\n            <span>候诊队列查询</span>\r\n          </template>\r\n        </el-menu-item>\r\n\r\n        <!-- 医院公告 -->\r\n        <el-menu-item index=\"/hospital-announcement\">\r\n          <template #title>\r\n            <i class=\"el-icon-bell\"></i>\r\n            <span>医院公告</span>\r\n          </template>\r\n        </el-menu-item>\r\n\r\n        <!-- 健康科普 -->\r\n        <el-menu-item index=\"/health-article\">\r\n          <template #title>\r\n            <i class=\"el-icon-view\"></i>\r\n            <span>健康科普</span>\r\n          </template>\r\n        </el-menu-item>\r\n      </el-menu>\r\n    </div>\r\n\r\n    <!-- 主体内容 -->\r\n    <div class=\"hospital-main\">\r\n      <!-- 侧边栏 -->\r\n      <div class=\"hospital-main-left\">\r\n\r\n      </div>\r\n\r\n      <!-- 数据表格 -->\r\n      <div class=\"hospital-main-right\">\r\n        <router-view @update:user=\"updateUser\"/>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 底部信息 -->\r\n    <div class=\"hospital-footer\">\r\n      <p>联系电话：010 - 20040520</p>\r\n      <p>地址：天津市 XX 区 找得到算你厉害</p>\r\n      <p>版权所有 &copy; 2025 沧秋sir</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"HospitalManager\",\r\n  data() {\r\n    return {\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n    };\r\n  },\r\n  created() {\r\n    if (!this.user.id) {\r\n      this.$router.push('/login');\r\n    }\r\n  },\r\n  methods: {\r\n    updateUser() {\r\n      this.user = JSON.parse(localStorage.getItem('xm-user') || '{}'); // 重新获取下用户的最新信息\r\n    },\r\n    goToPerson() {\r\n      if (this.user.role === 'ADMIN') {\r\n        this.$router.push('/adminPerson');\r\n      } else {\r\n        this.$router.push('/person'); // 普通用户跳转到个人信息页面\r\n      }\r\n    },\r\n    logout() {\r\n      localStorage.removeItem('xm-user'); // 清除用户信息\r\n      console.log('用户已退出登录'); // 调试日志\r\n      this.$router.push(\"/login\").catch(err => {\r\n        console.error('路由跳转失败:', err); // 捕获路由跳转错误\r\n      });\r\n    },\r\n    selectRegistration() {\r\n      this.$router.push(\"/selectRegistration\");\r\n    },\r\n    goToAppointmentRegistration(id) {\r\n      if (id = 1) {\r\n        this.$router.push({\r\n          path: '/Same-day-registration/reservation',\r\n          query: {\r\n            isAppointment: true\r\n          }\r\n        });\r\n      } else {\r\n        this.$router.push({\r\n          path: '/Same-day-registration/reservation',\r\n          query: {\r\n            isAppointment: false\r\n          }\r\n        })\r\n      }\r\n    },\r\n    goToArchieve() {\r\n      this.$router.push(\"/archives-management\");\r\n    },\r\n    goToWaitingQueue() {\r\n      this.$router.push(\"/Waiting-queues\")\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n@import \"@/assets/css/user-front.css\";\r\n\r\n.user-avatar {\r\n  cursor: pointer;\r\n  width: 40px;\r\n  height: 40px;\r\n  border-radius: 50%;\r\n}\r\n</style>"],"mappings":";AAgHA,eAAe;EACbA,IAAI,EAAE,iBAAiB;EACvBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAEC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,SAAS,KAAK,IAAI;IAC1D,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAAC,IAAI,CAACL,IAAI,CAACM,EAAE,EAAE;MACjB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;IAC7B;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACV,IAAG,GAAIC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,SAAS,KAAK,IAAI,CAAC,EAAE;IACnE,CAAC;;IACDO,UAAUA,CAAA,EAAG;MACX,IAAI,IAAI,CAACX,IAAI,CAACY,IAAG,KAAM,OAAO,