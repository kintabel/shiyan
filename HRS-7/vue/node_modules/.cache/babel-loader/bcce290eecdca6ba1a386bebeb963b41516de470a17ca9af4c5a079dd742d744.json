{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'Reservation',\n  data() {\n    return {\n      searchKeyword: '',\n      // 搜索关键词\n      hospitalList: [],\n      // 医院列表\n      isDialogVisible: false,\n      // 模态框是否显示\n      selectedHospital: {\n        // 当前选中的医院信息\n        name: '',\n        address: '',\n        phone: '',\n        level: '',\n        introduction: '',\n        departments: []\n      },\n      currentPage: 1,\n      // 当前页码\n      pageSize: 10,\n      // 每页显示条数\n      totalHospitals: 0,\n      // 总条数\n      isLoading: false // 加载状态\n    };\n  },\n\n  created() {\n    this.fetchHospitalList(1); // 初始化加载医院列表\n  },\n\n  methods: {\n    // 获取医院列表\n    fetchHospitalList(pageNumber) {\n      this.currentPage = pageNumber;\n      this.$request.get('/hospital/selectPage', {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          name: this.searchKeyword\n        }\n      }).then(response => {\n        if (response.data && response.data.list) {\n          this.hospitalList = response.data.list;\n        }\n        if (response.data && response.data.total) {\n          this.totalHospitals = response.data.total;\n        }\n      }).catch(error => {\n        console.error('获取医院列表失败:', error);\n        this.$message.error('获取医院列表失败，请稍后重试');\n      });\n    },\n    // 重置搜索\n    resetSearch() {\n      this.searchKeyword = '';\n      this.fetchHospitalList(1);\n    },\n    // 分页切换\n    handlePageChange(newPage) {\n      this.fetchHospitalList(newPage);\n    },\n    // 显示医院详情\n    showHospitalDetails(row) {\n      this.isLoading = true;\n      this.isDialogVisible = true;\n\n      // 通过表单提交的方式获取医院详情\n      this.$request.post('/hospital/selectAllData', {\n        id: row.id\n      }).then(response => {\n        if (response.data && response.data.data) {\n          this.selectedHospital = response.data.data;\n        } else {\n          console.error('获取医院详情数据格式错误:', response.data);\n          this.$message.error('获取医院详情失败，请稍后重试');\n        }\n      }).catch(error => {\n        console.error('获取医院详情失败:', error);\n        this.$message.error('获取医院详情失败，请稍后重试');\n      }).finally(() => {\n        this.isLoading = false;\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","searchKeyword","hospitalList","isDialogVisible","selectedHospital","address","phone","level","introduction","departments","currentPage","pageSize","totalHospitals","isLoading","created","fetchHospitalList","methods","pageNumber","$request","get","params","pageNum","then","response","list","total","catch","error","console","$message","resetSearch","handlePageChange","newPage","showHospitalDetails","row","post","id","finally"],"sources":["src/views/manager/Reservation.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- 搜索框 -->\r\n    <div class=\"search-container\">\r\n      <el-input v-model=\"searchKeyword\" placeholder=\"请输入医院名称\" @keyup.enter=\"fetchHospitalList(1)\"></el-input>\r\n      <el-button @click=\"fetchHospitalList(1)\">搜索</el-button>\r\n    </div>\r\n\r\n    <!-- 医院列表 -->\r\n    <el-table :data=\"hospitalList\" stripe @row-click=\"showHospitalDetails\">\r\n      <el-table-column prop=\"name\" label=\"医院名称\"></el-table-column>\r\n    </el-table>\r\n\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n        background\r\n        layout=\"prev, pager, next\"\r\n        :current-page=\"currentPage\"\r\n        :page-size=\"pageSize\"\r\n        :total=\"totalHospitals\"\r\n        @current-change=\"handlePageChange\"\r\n    ></el-pagination>\r\n\r\n    <!-- 医院详情模态框 -->\r\n    <el-dialog :visible.sync=\"isDialogVisible\" title=\"医院详情\">\r\n      <template #content>\r\n        <div v-if=\"isLoading\" class=\"loading-text\">加载中，请稍候...</div>\r\n        <div v-else>\r\n          <el-form :model=\"selectedHospital\" label-width=\"120px\">\r\n            <el-form-item label=\"医院名称\">\r\n              <el-input v-model=\"selectedHospital.name\" readonly></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"医院地址\">\r\n              <el-input v-model=\"selectedHospital.address\" readonly></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"联系电话\">\r\n              <el-input v-model=\"selectedHospital.phone\" readonly></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"医院等级\">\r\n              <el-input v-model=\"selectedHospital.level\" readonly></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"医院简介\">\r\n              <el-input type=\"textarea\" v-model=\"selectedHospital.introduction\" readonly></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"科室列表\">\r\n              <el-tag v-for=\"(department, index) in selectedHospital.departments\" :key=\"index\" style=\"margin-right: 10px;\">\r\n                {{ department }}\r\n              </el-tag>\r\n            </el-form-item>\r\n          </el-form>\r\n        </div>\r\n      </template>\r\n      <template #footer>\r\n        <el-button @click=\"isDialogVisible = false\">关闭</el-button>\r\n      </template>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'Reservation',\r\n  data() {\r\n    return {\r\n      searchKeyword: '', // 搜索关键词\r\n      hospitalList: [], // 医院列表\r\n      isDialogVisible: false, // 模态框是否显示\r\n      selectedHospital: { // 当前选中的医院信息\r\n        name: '',\r\n        address: '',\r\n        phone: '',\r\n        level: '',\r\n        introduction: '',\r\n        departments: []\r\n      },\r\n      currentPage: 1, // 当前页码\r\n      pageSize: 10, // 每页显示条数\r\n      totalHospitals: 0, // 总条数\r\n      isLoading: false // 加载状态\r\n    };\r\n  },\r\n  created() {\r\n    this.fetchHospitalList(1); // 初始化加载医院列表\r\n  },\r\n  methods: {\r\n    // 获取医院列表\r\n    fetchHospitalList(pageNumber) {\r\n      this.currentPage = pageNumber;\r\n      this.$request.get('/hospital/selectPage', {\r\n        params: {\r\n          pageNum: this.currentPage,\r\n          pageSize: this.pageSize,\r\n          name: this.searchKeyword\r\n        }\r\n      })\r\n          .then(response => {\r\n            if (response.data && response.data.list) {\r\n              this.hospitalList = response.data.list;\r\n            }\r\n            if (response.data && response.data.total) {\r\n              this.totalHospitals = response.data.total;\r\n            }\r\n          })\r\n          .catch(error => {\r\n            console.error('获取医院列表失败:', error);\r\n            this.$message.error('获取医院列表失败，请稍后重试');\r\n          });\r\n    },\r\n    // 重置搜索\r\n    resetSearch() {\r\n      this.searchKeyword = '';\r\n      this.fetchHospitalList(1);\r\n    },\r\n    // 分页切换\r\n    handlePageChange(newPage) {\r\n      this.fetchHospitalList(newPage);\r\n    },\r\n    // 显示医院详情\r\n    showHospitalDetails(row) {\r\n      this.isLoading = true;\r\n      this.isDialogVisible = true;\r\n\r\n      // 通过表单提交的方式获取医院详情\r\n      this.$request.post('/hospital/selectAllData', { id: row.id })\r\n          .then(response => {\r\n            if (response.data && response.data.data) {\r\n              this.selectedHospital = response.data.data;\r\n            } else {\r\n              console.error('获取医院详情数据格式错误:', response.data);\r\n              this.$message.error('获取医院详情失败，请稍后重试');\r\n            }\r\n          })\r\n          .catch(error => {\r\n            console.error('获取医院详情失败:', error);\r\n            this.$message.error('获取医院详情失败，请稍后重试');\r\n          })\r\n          .finally(() => {\r\n            this.isLoading = false;\r\n          });\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style scoped>\r\n.search-container {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.loading-text {\r\n  text-align: center;\r\n  font-size: 16px;\r\n  color: #666;\r\n}\r\n\r\n.el-form-item {\r\n  margin-bottom: 20px;\r\n}\r\n</style>"],"mappings":"AA4DA,OAAAA,KAAA;AAEA;EACAC,IAAA;