{"ast":null,"code":"export default {\n  name: 'LogisticsManagement',\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem(\"xm-user\") || '{}'),\n      loading: false,\n      logisticsList: [],\n      currentLogistics: null,\n      // 当前操作的物流记录\n      shipDialogVisible: false,\n      // 发货对话框显示状态\n      shipLoading: false,\n      // 发货操作加载状态\n      shipForm: {\n        outAddress: '',\n        outName: ''\n      },\n      shipRules: {\n        outAddress: [{\n          required: true,\n          message: '请输入发货地址',\n          trigger: 'blur'\n        }, {\n          min: 5,\n          max: 100,\n          message: '长度在 5 到 100 个字符',\n          trigger: 'blur'\n        }],\n        outName: [{\n          required: true,\n          message: '请输入发货人姓名',\n          trigger: 'blur'\n        }, {\n          min: 2,\n          max: 20,\n          message: '长度在 2 到 20 个字符',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  methods: {\n    refreshLogistics() {\n      this.getLogisticsList();\n    },\n    getLogisticsList() {\n      this.loading = true;\n      this.$request.get('/admin/logistics', {\n        params: {\n          adminId: this.user.id\n        }\n      }).then(response => {\n        if (response.code == 200) {\n          this.logisticsList = response.data;\n        } else {\n          this.$message.error(response.data.message || '获取物流信息失败');\n        }\n      }).catch(error => {\n        console.error('获取物流信息失败:', error);\n        this.$message.error('请求失败，请稍后重试');\n      }).finally(() => {\n        this.loading = false;\n      });\n    },\n    formatPhone(phone) {\n      if (phone && phone.length === 11) {\n        return phone.substring(0, 3) + '****' + phone.substring(7);\n      }\n      return phone;\n    },\n    getStatusText(status) {\n      switch (status) {\n        case 0:\n          return '待发货';\n        case 1:\n          return '已发货';\n        default:\n          return '未知状态';\n      }\n    },\n    // 显示发货对话框\n    showShipDialog(logistics) {\n      this.currentLogistics = logistics;\n      this.shipForm = {\n        outAddress: '',\n        outName: ''\n      };\n      this.shipDialogVisible = true;\n      this.$nextTick(() => {\n        this.$refs.shipForm.clearValidate();\n      });\n    },\n    // 处理发货操作\n    handleShip() {\n      this.$refs.shipForm.validate(valid => {\n        if (valid) {\n          this.shipLoading = true;\n          console.log(this.currentLogistics.id, this.shipForm.outAddress, this.shipForm.outName);\n          this.$request.post('admin/logistics/ship', null, {\n            params: {\n              orderId: this.currentLogistics.id,\n              outAddress: this.shipForm.outAddress,\n              outName: this.shipForm.outName\n            }\n          }).then(response => {\n            if (response.code == 200) {\n              this.$message.success('发货成功');\n              this.shipDialogVisible = false;\n              this.getLogisticsList(); // 刷新列表\n            } else {\n              this.$message.error(response.message || '发货失败');\n            }\n          }).catch(error => {\n            console.error('发货失败:', error);\n            this.$message.error('发货失败，请稍后重试');\n          }).finally(() => {\n            this.shipLoading = false;\n          });\n        }\n      });\n    }\n  },\n  mounted() {\n    this.getLogisticsList();\n  }\n};","map":{"version":3,"names":["name","data","user","JSON","parse","localStorage","getItem","loading","logisticsList","currentLogistics","shipDialogVisible","shipLoading","shipForm","outAddress","outName","shipRules","required","message","trigger","min","max","methods","refreshLogistics","getLogisticsList","$request","get","params","adminId","id","then","response","code","$message","error","catch","console","finally","formatPhone","phone","length","substring","getStatusText","status","showShipDialog","logistics","$nextTick","$refs","clearValidate","handleShip","validate","valid","log","post","orderId","success","mounted"],"sources":["E:\\专业设计项目\\HRS\\HRS-7\\vue\\src\\views\\hospitalAdmin\\Logistics.vue"],"sourcesContent":["<template>\r\n  <div class=\"app-container\">\r\n    <el-card>\r\n      <div slot=\"header\" class=\"clearfix\">\r\n        <span>物流信息管理</span>\r\n        <div style=\"float: right\">\r\n          <el-button type=\"primary\" @click=\"refreshLogistics\" :loading=\"loading\">\r\n            刷新列表\r\n          </el-button>\r\n        </div>\r\n      </div>\r\n\r\n      <el-table\r\n          v-loading=\"loading\"\r\n          :data=\"logisticsList\"\r\n          border\r\n          style=\"width: 100%\"\r\n      >\r\n        <el-table-column prop=\"order\" label=\"订单号\" width=\"180\" />\r\n        <el-table-column prop=\"userName\" label=\"收货人\" width=\"120\" />\r\n        <el-table-column label=\"联系电话\" width=\"150\">\r\n          <template #default=\"scope\">\r\n            {{ formatPhone(scope.row.phone) }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"inAddress\" label=\"收货地址\" />\r\n        <el-table-column prop=\"applicationTime\" label=\"申请时间\" width=\"180\" />\r\n        <el-table-column label=\"状态\" width=\"120\">\r\n          <template #default=\"scope\">\r\n            {{ getStatusText(scope.row.status) }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\" width=\"120\">\r\n          <template #default=\"scope\">\r\n            <el-button\r\n                v-if=\"scope.row.status === 0\"\r\n                size=\"mini\"\r\n                type=\"primary\"\r\n                @click=\"showShipDialog(scope.row)\"\r\n            >\r\n              发货\r\n            </el-button>\r\n            <span v-else-if=\"scope.row.status === 1\">\r\n              已发货\r\n            </span>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-card>\r\n\r\n    <!-- 发货对话框 -->\r\n    <el-dialog title=\"发货信息\" :visible.sync=\"shipDialogVisible\" width=\"50%\">\r\n      <el-form :model=\"shipForm\" :rules=\"shipRules\" ref=\"shipForm\" label-width=\"100px\">\r\n        <el-form-item label=\"发货地址\" prop=\"outAddress\">\r\n          <el-input v-model=\"shipForm.outAddress\" placeholder=\"请输入发货地址\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"发货人\" prop=\"outName\">\r\n          <el-input v-model=\"shipForm.outName\" placeholder=\"请输入发货人姓名\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"shipDialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"handleShip\" :loading=\"shipLoading\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'LogisticsManagement',\r\n  data() {\r\n    return {\r\n      user: JSON.parse(localStorage.getItem(\"xm-user\") || '{}'),\r\n      loading: false,\r\n      logisticsList: [],\r\n      currentLogistics: null, // 当前操作的物流记录\r\n      shipDialogVisible: false, // 发货对话框显示状态\r\n      shipLoading: false, // 发货操作加载状态\r\n      shipForm: {\r\n        outAddress: '',\r\n        outName: ''\r\n      },\r\n      shipRules: {\r\n        outAddress: [\r\n          { required: true, message: '请输入发货地址', trigger: 'blur' },\r\n          { min: 5, max: 100, message: '长度在 5 到 100 个字符', trigger: 'blur' }\r\n        ],\r\n        outName: [\r\n          { required: true, message: '请输入发货人姓名', trigger: 'blur' },\r\n          { min: 2, max: 20, message: '长度在 2 到 20 个字符', trigger: 'blur' }\r\n        ]\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    refreshLogistics() {\r\n      this.getLogisticsList();\r\n    },\r\n\r\n    getLogisticsList() {\r\n      this.loading = true;\r\n      this.$request.get('/admin/logistics', {\r\n        params: {\r\n          adminId: this.user.id\r\n        }\r\n      })\r\n          .then(response => {\r\n            if (response.code == 200) {\r\n              this.logisticsList = response.data;\r\n            } else {\r\n              this.$message.error(response.data.message || '获取物流信息失败');\r\n            }\r\n          })\r\n          .catch(error => {\r\n            console.error('获取物流信息失败:', error);\r\n            this.$message.error('请求失败，请稍后重试');\r\n          })\r\n          .finally(() => {\r\n            this.loading = false;\r\n          });\r\n    },\r\n\r\n    formatPhone(phone) {\r\n      if (phone && phone.length === 11) {\r\n        return phone.substring(0, 3) + '****' + phone.substring(7);\r\n      }\r\n      return phone;\r\n    },\r\n\r\n    getStatusText(status) {\r\n      switch (status) {\r\n        case 0: return '待发货'\r\n        case 1: return '已发货'\r\n        default: return '未知状态'\r\n      }\r\n    },\r\n\r\n    // 显示发货对话框\r\n    showShipDialog(logistics) {\r\n      this.currentLogistics = logistics;\r\n      this.shipForm = {\r\n        outAddress: '',\r\n        outName: ''\r\n      };\r\n      this.shipDialogVisible = true;\r\n      this.$nextTick(() => {\r\n        this.$refs.shipForm.clearValidate();\r\n      });\r\n    },\r\n\r\n    // 处理发货操作\r\n    handleShip() {\r\n      this.$refs.shipForm.validate(valid => {\r\n        if (valid) {\r\n          this.shipLoading = true;\r\n          console.log(this.currentLogistics.id, this.shipForm.outAddress, this.shipForm.outName)\r\n          this.$request.post('admin/logistics/ship', null,{\r\n            params:{\r\n              orderId: this.currentLogistics.id,\r\n              outAddress: this.shipForm.outAddress,\r\n              outName: this.shipForm.outName\r\n            }\r\n          })\r\n              .then(response => {\r\n                if (response.code == 200) {\r\n                  this.$message.success('发货成功');\r\n                  this.shipDialogVisible = false;\r\n                  this.getLogisticsList(); // 刷新列表\r\n                } else {\r\n                  this.$message.error(response.message || '发货失败');\r\n                }\r\n              })\r\n              .catch(error => {\r\n                console.error('发货失败:', error);\r\n                this.$message.error('发货失败，请稍后重试');\r\n              })\r\n              .finally(() => {\r\n                this.shipLoading = false;\r\n              });\r\n        }\r\n      });\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getLogisticsList();\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.clearfix:before,\r\n.clearfix:after {\r\n  display: table;\r\n  content: \"\";\r\n}\r\n.clearfix:after {\r\n  clear: both;\r\n}\r\n\r\n.app-container {\r\n  padding: 20px;\r\n}\r\n\r\n.el-table {\r\n  margin-top: 10px;\r\n}\r\n</style>"],"mappings":"AAqEA,eAAe;EACbA,IAAI,EAAE,qBAAqB;EAC3BC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAEC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,SAAS,KAAK,IAAI,CAAC;MACzDC,OAAO,EAAE,KAAK;MACdC,aAAa,EAAE,EAAE;MACjBC,gBAAgB,EAAE,IAAI;MAAE;MACxBC,iBAAiB,EAAE,KAAK;MAAE;MAC1BC,WAAW,EAAE,KAAK;MAAE;MACpBC,QAAQ,EAAE;QACRC,UAAU,EAAE,EAAE;QACdC,OAAO,EAAE;MACX,CAAC;MACDC,SAAS,EAAE;QACTF,UAAU,EAAE,CACV;UAAEG,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,SAAS;UAAEC,OAAO,EAAE;QAAO,CAAC,EACvD;UAAEC,GAAG,EAAE,CAAC;UAAEC,GAAG,EAAE,GAAG;UAAEH,OAAO,EAAE,iBAAiB;UAAEC,OAAO,EAAE;QAAO,EACjE;QACDJ,OAAO,EAAE,CACP;UAAEE,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,UAAU;UAAEC,OAAO,EAAE;QAAO,CAAC,EACxD;UAAEC,GAAG,EAAE,CAAC;UAAEC,GAAG,EAAE,EAAE;UAAEH,OAAO,EAAE,gBAAgB;UAAEC,OAAO,EAAE;QAAO;MAElE;IACF,CAAC;EACH,CAAC;EACDG,OAAO,EAAE;IACPC,gBAAgBA,CAAA,EAAG;MACjB,IAAI,CAACC,gBAAgB,CAAC,CAAC;IACzB,CAAC;IAEDA,gBAAgBA,CAAA,EAAG;MACjB,IAAI,CAAChB,OAAM,GAAI,IAAI;MACnB,IAAI,CAACiB,QAAQ,CAACC,GAAG,CAAC,kBAAkB,EAAE;QACpCC,MAAM,EAAE;UACNC,OAAO,EAAE,IAAI,CAACzB,IAAI,CAAC0B;QACrB;MACF,CAAC,EACIC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIA,QAAQ,CAACC,IAAG,IAAK,GAAG,EAAE;UACxB,IAAI,CAACvB,aAAY,GAAIsB,QAAQ,CAAC7B,IAAI;QACpC,OAAO;UACL,IAAI,CAAC+B,QAAQ,CAACC,KAAK,CAACH,QAAQ,CAAC7B,IAAI,CAACgB,OAAM,IAAK,UAAU,CAAC;QAC1D;MACF,CAAC,EACAiB,KAAK,CAACD,KAAI,IAAK;QACdE,OAAO,CAACF,KAAK,CAAC,W