{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'Reservation',\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      searchKeyword: '',\n      // 搜索关键词\n      hospitalList: [],\n      // 医院列表\n      currentPage: 1,\n      // 当前页码\n      pageSize: 10,\n      // 每页显示条数\n      totalHospitals: 0 // 总条数\n    };\n  },\n\n  created() {\n    this.fetchHospitalList(1); // 初始化加载医院列表\n  },\n\n  methods: {\n    // 获取医院列表\n    fetchHospitalList(pageNumber) {\n      this.currentPage = pageNumber;\n      this.$request.get('/hospital/selectPage', {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          name: this.searchKeyword\n        }\n      }).then(response => {\n        if (response.data && response.data.list) {\n          this.hospitalList = response.data.list;\n        }\n        if (response.data && response.data.total) {\n          this.totalHospitals = response.data.total;\n        }\n      }).catch(error => {\n        console.error('获取医院列表失败:', error);\n        this.$message.error('获取医院列表失败，请稍后重试');\n      });\n    },\n    // 重置搜索\n    resetSearch() {\n      this.searchKeyword = '';\n      this.fetchHospitalList(1);\n    },\n    // 分页切换\n    handlePageChange(newPage) {\n      this.fetchHospitalList(newPage);\n    },\n    // 跳转到医院详情页面\n    goToHospitalDetail(hospitalId) {\n      this.$router.push(`/appointment-registration/hospital/${hospitalId}`);\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","user","JSON","parse","localStorage","getItem","searchKeyword","hospitalList","currentPage","pageSize","totalHospitals","created","fetchHospitalList","methods","pageNumber","$request","get","params","pageNum","then","response","list","total","catch","error","console","$message","resetSearch","handlePageChange","newPage","goToHospitalDetail","hospitalId","$router","push"],"sources":["src/views/user/Reservation.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- 搜索框 -->\r\n    <div class=\"search-container\">\r\n      <el-input v-model=\"searchKeyword\" placeholder=\"请输入医院名称\" @keyup.enter=\"fetchHospitalList(1)\"></el-input>\r\n      <el-button @click=\"fetchHospitalList(1)\">搜索</el-button>\r\n    </div>\r\n\r\n    <!-- 医院列表 -->\r\n    <el-table :data=\"hospitalList\" stripe>\r\n      <el-table-column prop=\"name\" label=\"医院名称\">\r\n        <template slot-scope=\"scope\">\r\n          <el-link type=\"primary\" @click=\"goToHospitalDetail(scope.row.id)\">{{ scope.row.name }}</el-link>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n        background\r\n        layout=\"prev, pager, next\"\r\n        :current-page=\"currentPage\"\r\n        :page-size=\"pageSize\"\r\n        :total=\"totalHospitals\"\r\n        @current-change=\"handlePageChange\"\r\n    ></el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'Reservation',\r\n  data() {\r\n    return {\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      searchKeyword: '', // 搜索关键词\r\n      hospitalList: [], // 医院列表\r\n      currentPage: 1, // 当前页码\r\n      pageSize: 10, // 每页显示条数\r\n      totalHospitals: 0 // 总条数\r\n    };\r\n  },\r\n  created() {\r\n    this.fetchHospitalList(1); // 初始化加载医院列表\r\n  },\r\n  methods: {\r\n    // 获取医院列表\r\n    fetchHospitalList(pageNumber) {\r\n      this.currentPage = pageNumber;\r\n      this.$request.get('/hospital/selectPage', {\r\n        params: {\r\n          pageNum: this.currentPage,\r\n          pageSize: this.pageSize,\r\n          name: this.searchKeyword\r\n        }\r\n      })\r\n          .then(response => {\r\n            if (response.data && response.data.list) {\r\n              this.hospitalList = response.data.list;\r\n            }\r\n            if (response.data && response.data.total) {\r\n              this.totalHospitals = response.data.total;\r\n            }\r\n          })\r\n          .catch(error => {\r\n            console.error('获取医院列表失败:', error);\r\n            this.$message.error('获取医院列表失败，请稍后重试');\r\n          });\r\n    },\r\n    // 重置搜索\r\n    resetSearch() {\r\n      this.searchKeyword = '';\r\n      this.fetchHospitalList(1);\r\n    },\r\n    // 分页切换\r\n    handlePageChange(newPage) {\r\n      this.fetchHospitalList(newPage);\r\n    },\r\n    // 跳转到医院详情页面\r\n    goToHospitalDetail(hospitalId) {\r\n      this.$router.push(`/appointment-registration/hospital/${hospitalId}`);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.search-container {\r\n  margin-bottom: 20px;\r\n}\r\n</style>"],"mappings":";AA8BA,OAAAA,KAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,aAAA;MAAA;MACAC,YAAA;MAAA;MACAC,WAAA;MAAA;MACAC,QAAA;MAAA;MACAC,cAAA;IACA;EACA;;EACAC,QAAA;IACA,KAAAC,iBAAA;EACA;;EACAC,OAAA;IACA;IACAD,kBAAAE,UAAA;MACA,KAAAN,WAAA,GAAAM,UAAA;MACA,KAAAC,QAAA,CAAAC,GAAA;QACAC,MAAA;UACAC,OAAA,OAAAV,WAAA;UACAC,QAAA,OAAAA,QAAA;UACAV,IAAA,OAAAO;QACA;MACA,GACAa,IAAA,CAAAC,QAAA;QACA,IAAAA,QAAA,CAAApB,IAAA,IAAAoB,QAAA,CAAApB,IAAA,CAAAqB,IAAA;UACA,KAAAd,YAAA,GAAAa,QAAA,CAAApB,IAAA,CAAAqB,IAAA;QACA;QACA,IAAAD,QAAA,CAAApB,IAAA,IAAAoB,QAAA,CAAApB,IAAA,CAAAsB,KAAA;UACA,KAAAZ,cAAA,GAAAU,QAAA,CAAApB,IAAA,CAAAsB,KAAA;QACA;MACA,GACAC,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,cAAAA,KAAA;QACA,KAAAE,QAAA,CAAAF,KAAA;MACA;IACA;IACA;IACAG,YAAA;MACA,KAAArB,aAAA;MACA,KAAAM,iBAAA;IACA;IACA;IACAgB,iBAAAC,OAAA;MACA,KAAAjB,iBAAA,CAAAiB,OAAA;IACA;IACA;IACAC,mBAAAC,UAAA;MACA,KAAAC,OAAA,CAAAC,IAAA,uCAAAF,UAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}