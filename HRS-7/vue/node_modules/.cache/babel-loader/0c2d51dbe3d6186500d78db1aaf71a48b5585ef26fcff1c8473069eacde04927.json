{"ast":null,"code":"export default {\n  name: 'ArticleDetail',\n  data() {\n    return {\n      article: {\n        title: '',\n        content: '',\n        category: '',\n        publishTime: '',\n        viewCount: 0,\n        likeCount: 0\n      },\n      likeLoading: false\n    };\n  },\n  methods: {\n    getArticleDetail() {\n      this.loading = true;\n      this.$request.get('/user/getHealthArticle', {\n        params: {\n          id: this.$route.params.id\n        }\n      }).then(response => {\n        if (response.code == 200) {\n          this.article = response.data;\n        }\n      }).finally(() => {\n        this.loading = false;\n      });\n    },\n    likeArticle() {\n      this.likeLoading = true;\n      const id = this.$route.params.id;\n      this.$request.post(`/user/likeArticle/${id}`).then(response => {\n        if (response.code == 200) {\n          this.article.likeCount++;\n          this.$message.success('点赞成功');\n        }\n      }).finally(() => {\n        this.likeLoading = false;\n      });\n    }\n  },\n  mounted() {\n    this.getArticleDetail();\n  }\n};","map":{"version":3,"names":["name","data","article","title","content","category","publishTime","viewCount","likeCount","likeLoading","methods","getArticleDetail","loading","$request","get","params","id","$route","then","response","code","finally","likeArticle","post","$message","success","mounted"],"sources":["E:\\专业设计项目\\HRS\\HRS-7\\vue\\src\\views\\user\\HealthArticleDetail.vue"],"sourcesContent":["<template>\r\n  <div class=\"app-container article-detail\">\r\n    <el-card>\r\n      <div slot=\"header\" class=\"clearfix\">\r\n        <el-button\r\n            type=\"text\"\r\n            icon=\"el-icon-arrow-left\"\r\n            @click=\"$router.go(-1)\"\r\n        >返回文章列表</el-button>\r\n      </div>\r\n\r\n      <div class=\"article-header\">\r\n        <h1 class=\"article-title\">{{ article.title }}</h1>\r\n        <div class=\"article-meta\">\r\n          <span class=\"category\">{{ article.category }}</span>\r\n          <span class=\"publish-time\">{{ article.publishTime }}</span>\r\n          <span class=\"view-count\">\r\n            <i class=\"el-icon-view\"></i> {{ article.viewCount }} 阅读\r\n          </span>\r\n          <span class=\"like-count\">\r\n            <i class=\"el-icon-star-off\"></i> {{ article.likeCount }} 点赞\r\n          </span>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"article-content\" v-html=\"article.content\"></div>\r\n\r\n      <div class=\"article-footer\">\r\n        <el-button\r\n            type=\"primary\"\r\n            icon=\"el-icon-thumb\"\r\n            @click=\"likeArticle\"\r\n            :loading=\"likeLoading\"\r\n        >点赞</el-button>\r\n      </div>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'ArticleDetail',\r\n  data() {\r\n    return {\r\n      article: {\r\n        title: '',\r\n        content: '',\r\n        category: '',\r\n        publishTime: '',\r\n        viewCount: 0,\r\n        likeCount: 0\r\n      },\r\n      likeLoading: false\r\n    };\r\n  },\r\n  methods: {\r\n    getArticleDetail() {\r\n      this.loading = true;\r\n      this.$request.get('/user/getHealthArticle', {\r\n        params: { id: this.$route.params.id }\r\n      })\r\n          .then(response => {\r\n            if (response.code == 200) {\r\n              this.article = response.data;\r\n            }\r\n          })\r\n          .finally(() => {\r\n            this.loading = false;\r\n          });\r\n    },\r\n    likeArticle() {\r\n      this.likeLoading = true;\r\n      const id = this.$route.params.id;\r\n      this.$request.post(`/user/likeArticle/${id}`)\r\n          .then(response => {\r\n            if (response.code == 200) {\r\n              this.article.likeCount++;\r\n              this.$message.success('点赞成功');\r\n            }\r\n          })\r\n          .finally(() => {\r\n            this.likeLoading = false;\r\n          });\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getArticleDetail();\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.article-detail {\r\n  max-width: 900px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.article-header {\r\n  margin-bottom: 30px;\r\n  border-bottom: 1px solid #eee;\r\n  padding-bottom: 20px;\r\n}\r\n\r\n.article-title {\r\n  font-size: 28px;\r\n  color: #333;\r\n  margin-bottom: 15px;\r\n  line-height: 1.4;\r\n}\r\n\r\n.article-meta {\r\n  color: #999;\r\n  font-size: 14px;\r\n}\r\n\r\n.article-meta span {\r\n  margin-right: 20px;\r\n}\r\n\r\n.article-meta i {\r\n  margin-right: 5px;\r\n}\r\n\r\n.article-content {\r\n  line-height: 1.8;\r\n  font-size: 16px;\r\n  color: #333;\r\n}\r\n\r\n.article-content >>> p {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.article-footer {\r\n  margin-top: 40px;\r\n  text-align: center;\r\n  padding-top: 20px;\r\n  border-top: 1px solid #eee;\r\n}\r\n</style>"],"mappings":"AAwCA,eAAe;EACbA,IAAI,EAAE,eAAe;EACrBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;QACPC,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE,EAAE;QACXC,QAAQ,EAAE,EAAE;QACZC,WAAW,EAAE,EAAE;QACfC,SAAS,EAAE,CAAC;QACZC,SAAS,EAAE;MACb,CAAC;MACDC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,gBAAgBA,CAAA,EAAG;MACjB,IAAI,CAACC,OAAM,GAAI,IAAI;MACnB,IAAI,CAACC,QAAQ,CAACC,GAAG,CAAC,wBAAwB,EAAE;QAC1CC,MAAM,EAAE;UAAEC,EAAE,EAAE,IAAI,CAACC,MAAM,CAACF,MAAM,CAACC;QAAG;MACtC,CAAC,EACIE,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIA,QAAQ,CAACC,IAAG,IAAK,GAAG,EAAE;UACxB,IAAI,CAAClB,OAAM,GAAIiB,QAAQ,CAAClB,IAAI;QAC9B;MACF,CAAC,EACAoB,OAAO,CAAC,MAAM;QACb,IAAI,CAACT,OAAM,GAAI,KAAK;MACtB,CAAC,CAAC;IACR,CAAC;IACDU,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACb,WAAU,GAAI,IAAI;MACvB,MAAMO,EAAC,GAAI,IAAI,CAACC,MAAM,CAACF,MAAM,CAACC,EAAE;MAChC,IAAI,CAACH,QAAQ,CAACU,IAAI,CAAE,qBAAoBP,EAAG,EAAC,EACvCE,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIA,QAAQ,CAACC,IAAG,IAAK,GAAG,EAAE;UACxB,IAAI,CAAClB,OAAO,CAACM,SAAS,EAAE;UACxB,IAAI,CAACgB,QAAQ,CAACC,OAAO,CAAC,MAAM,CAAC;QAC/B;MACF,CAAC,EACAJ,OAAO,CAAC,MAAM;QACb,IAAI,CAACZ,WAAU,GAAI,KAAK;MAC1B,CAAC,CAAC;I