{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'Reservation',\n  data() {\n    return {\n      searchKeyword: '',\n      // 搜索关键词\n      hospitalList: [],\n      // 医院列表\n      dialogVisible: false,\n      // 模态框是否显示\n      selectedHospital: {},\n      // 当前选中的医院信息\n      tableData: [],\n      // 所有的数据\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 10,\n      // 每页显示的个数\n      total: 0,\n      // 总条数\n      name: null,\n      // 搜索条件\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      ids: [],\n      loading: false // 加载状态\n    };\n  },\n\n  created() {\n    this.getHospitalList(1);\n  },\n  methods: {\n    getHospitalList(pageNum) {\n      // 分页查询\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/hospital/selectPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          name: this.searchKeyword // 使用搜索关键词\n        }\n      }).then(res => {\n        if (res.data && res.data.list) {\n          this.hospitalList = res.data.list; // 更新医院列表\n        } else {\n          console.error('获取医院列表数据格式错误:', res.data);\n          this.$message.error('获取医院列表失败，请稍后重试');\n        }\n        if (res.data && res.data.total) {\n          this.total = res.data.total; // 更新总条数\n        } else {\n          console.error('获取总条数数据格式错误:', res.data);\n          this.$message.error('获取总条数失败，请稍后重试');\n        }\n      }).catch(error => {\n        console.error('获取医院列表失败:', error);\n        console.error('错误响应:', error.response ? error.response.data : '无响应数据');\n        this.$message.error('获取医院列表失败，请稍后重试');\n      });\n    },\n    reset() {\n      this.searchKeyword = null; // 重置搜索关键词\n      this.getHospitalList(1); // 重新加载数据\n    },\n\n    handleCurrentChange(pageNum) {\n      this.getHospitalList(pageNum); // 分页切换\n    },\n\n    showHospitalDetail(row) {\n      this.loading = true; // 显示加载状态\n      this.dialogVisible = true; // 显示模态框\n\n      // 调用新接口获取医院详情\n      this.$request.get('/hospital/selectAllData', {\n        params: {\n          id: row.id // 假设 row 中有医院的唯一标识 id\n        }\n      }).then(res => {\n        if (res.data && res.data.data) {\n          const data = res.data.data;\n          // 进行数据类型检查\n          if (typeof data.name === 'string' && typeof data.address === 'string' && typeof data.phone === 'string' && typeof data.level === 'number' && Array.isArray(data.departments) && typeof data.introduction === 'string') {\n            this.$nextTick(() => {\n              this.selectedHospital = data;\n            });\n          } else {\n            console.error('医院详情数据类型错误:', data);\n            this.$message.error('数据格式不正确，无法显示详情');\n          }\n        } else {\n          console.error('获取医院详情接口返回数据格式错误:', res.data);\n          this.$message.error('获取医院详情失败，请稍后重试');\n        }\n      }).catch(error => {\n        console.error('获取医院详情失败:', error);\n        console.error('错误响应:', error.response ? error.response.data : '无响应数据');\n        this.$message.error('获取医院详情失败，请稍后重试');\n      }).finally(() => {\n        this.loading = false; // 关闭加载状态\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","searchKeyword","hospitalList","dialogVisible","selectedHospital","tableData","pageNum","pageSize","total","user","JSON","parse","localStorage","getItem","ids","loading","created","getHospitalList","methods","$request","get","params","then","res","list","console","error","$message","catch","response","reset","handleCurrentChange","showHospitalDetail","row","id","address","phone","level","Array","isArray","departments","introduction","$nextTick","finally"],"sources":["src/views/manager/Reservation.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- 搜索框 -->\r\n    <div class=\"search-container\">\r\n      <el-input v-model=\"searchKeyword\" placeholder=\"请输入医院名称\" @keyup.enter=\"getHospitalList(1)\"></el-input>\r\n      <el-button @click=\"getHospitalList(1)\">搜索</el-button>\r\n    </div>\r\n\r\n    <!-- 医院列表 -->\r\n    <el-table :data=\"hospitalList\" stripe @row-click=\"showHospitalDetail\">\r\n      <el-table-column prop=\"name\" label=\"医院名称\"></el-table-column>\r\n      <!-- 可以根据实际情况添加更多列 -->\r\n    </el-table>\r\n\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n        background\r\n        layout=\"prev, pager, next\"\r\n        :current-page=\"pageNum\"\r\n        :page-size=\"pageSize\"\r\n        :total=\"total\"\r\n        @current-change=\"handleCurrentChange\"\r\n    ></el-pagination>\r\n\r\n    <!-- 医院详情模态框 -->\r\n    <el-dialog :visible.sync=\"dialogVisible\" title=\"医院详情\">\r\n      <template #content>\r\n        <!-- 加载状态 -->\r\n        <div v-if=\"loading\" class=\"loading-text\">加载中，请稍候...</div>\r\n        <div v-else>\r\n          <p v-if=\"selectedHospital.name\"><strong>医院名称:</strong> {{ selectedHospital.name }}</p>\r\n          <p v-if=\"selectedHospital.address\"><strong>医院地址:</strong> {{ selectedHospital.address }}</p>\r\n          <p v-if=\"selectedHospital.phone\"><strong>联系电话:</strong> {{ selectedHospital.phone }}</p>\r\n          <p v-if=\"selectedHospital.level\"><strong>医院等级:</strong> {{ selectedHospital.level }}</p>\r\n          <p v-if=\"selectedHospital.introduction\"><strong>医院简介:</strong> {{ selectedHospital.introduction }}</p>\r\n          <p v-if=\"Array.isArray(selectedHospital.departments) && selectedHospital.departments.length > 0\"><strong>科室列表:</strong></p>\r\n          <ul v-if=\"Array.isArray(selectedHospital.departments)\">\r\n            <li v-for=\"(department, index) in selectedHospital.departments\" :key=\"index\">\r\n              {{ department }}\r\n            </li>\r\n          </ul>\r\n        </div>\r\n      </template>\r\n      <template #footer>\r\n        <el-button @click=\"dialogVisible = false\">关闭</el-button>\r\n      </template>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'Reservation',\r\n  data() {\r\n    return {\r\n      searchKeyword: '', // 搜索关键词\r\n      hospitalList: [], // 医院列表\r\n      dialogVisible: false, // 模态框是否显示\r\n      selectedHospital: {}, // 当前选中的医院信息\r\n      tableData: [], // 所有的数据\r\n      pageNum: 1, // 当前的页码\r\n      pageSize: 10, // 每页显示的个数\r\n      total: 0, // 总条数\r\n      name: null, // 搜索条件\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      ids: [],\r\n      loading: false, // 加载状态\r\n    };\r\n  },\r\n  created() {\r\n    this.getHospitalList(1);\r\n  },\r\n  methods: {\r\n    getHospitalList(pageNum) {  // 分页查询\r\n      if (pageNum) this.pageNum = pageNum;\r\n      this.$request.get('/hospital/selectPage', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          name: this.searchKeyword, // 使用搜索关键词\r\n        }\r\n      }).then(res => {\r\n        if (res.data && res.data.list) {\r\n          this.hospitalList = res.data.list; // 更新医院列表\r\n        } else {\r\n          console.error('获取医院列表数据格式错误:', res.data);\r\n          this.$message.error('获取医院列表失败，请稍后重试');\r\n        }\r\n        if (res.data && res.data.total) {\r\n          this.total = res.data.total; // 更新总条数\r\n        } else {\r\n          console.error('获取总条数数据格式错误:', res.data);\r\n          this.$message.error('获取总条数失败，请稍后重试');\r\n        }\r\n      }).catch(error => {\r\n        console.error('获取医院列表失败:', error);\r\n        console.error('错误响应:', error.response? error.response.data : '无响应数据');\r\n        this.$message.error('获取医院列表失败，请稍后重试');\r\n      });\r\n    },\r\n    reset() {\r\n      this.searchKeyword = null; // 重置搜索关键词\r\n      this.getHospitalList(1); // 重新加载数据\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.getHospitalList(pageNum); // 分页切换\r\n    },\r\n    showHospitalDetail(row) {\r\n      this.loading = true; // 显示加载状态\r\n      this.dialogVisible = true; // 显示模态框\r\n\r\n      // 调用新接口获取医院详情\r\n      this.$request.get('/hospital/selectAllData', {\r\n        params: {\r\n          id: row.id, // 假设 row 中有医院的唯一标识 id\r\n        }\r\n      }).then(res => {\r\n        if (res.data && res.data.data) {\r\n          const data = res.data.data;\r\n          // 进行数据类型检查\r\n          if (typeof data.name ==='string' && typeof data.address ==='string' && typeof data.phone ==='string' && typeof data.level === 'number' && Array.isArray(data.departments) && typeof data.introduction ==='string') {\r\n            this.$nextTick(() => {\r\n              this.selectedHospital = data;\r\n            });\r\n          } else {\r\n            console.error('医院详情数据类型错误:', data);\r\n            this.$message.error('数据格式不正确，无法显示详情');\r\n          }\r\n        } else {\r\n          console.error('获取医院详情接口返回数据格式错误:', res.data);\r\n          this.$message.error('获取医院详情失败，请稍后重试');\r\n        }\r\n      }).catch(error => {\r\n        console.error('获取医院详情失败:', error);\r\n        console.error('错误响应:', error.response? error.response.data : '无响应数据');\r\n        this.$message.error('获取医院详情失败，请稍后重试');\r\n      }).finally(() => {\r\n        this.loading = false; // 关闭加载状态\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.search-container {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.loading-text {\r\n  text-align: center;\r\n  font-size: 16px;\r\n  color: #666;\r\n}\r\n</style>"],"mappings":"AAmDA,OAAAA,KAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,aAAA;MAAA;MACAC,YAAA;MAAA;MACAC,aAAA;MAAA;MACAC,gBAAA;MAAA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MAAA;MACAT,IAAA;MAAA;MACAU,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,GAAA;MACAC,OAAA;IACA;EACA;;EACAC,QAAA;IACA,KAAAC,eAAA;EACA;EACAC,OAAA;IACAD,gBAAAX,OAAA;MAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAa,QAAA,CAAAC,GAAA;QACAC,MAAA;UACAf,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAR,IAAA,OAAAE,aAAA;QACA;MACA,GAAAqB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAvB,IAAA,IAAAuB,GAAA,CAAAvB,IAAA,CAAAwB,IAAA;UACA,KAAAtB,YAAA,GAAAqB,GAAA,CAAAvB,IAAA,CAAAwB,IAAA;QACA;UACAC,OAAA,CAAAC,KAAA,kBAAAH,GAAA,