) {\r\n    return {\r\n      userInfo: null,\r\n      isLoading: true,\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n    }\r\n  },\r\n\r\n  created() {\r\n    const userId = this.user.id;\r\n    this.fetchUser(userId);\r\n  },\r\n  methods: {\r\n\r\n\r\n    async fetchUser(userId) {\r\n      this.$request.get(`/user/selectById/${userId}`)\r\n          .then(response => {\r\n            if (response.data) {\r\n              this.userInfo = response.data\r\n            } else {\r\n              this.handleError('获取用户详细信息失败', response.data)\r\n            }\r\n          })\r\n          .catch(error => {\r\n            this.handleError('获取用户详细信息失败', error)\r\n          })\r\n          .finally(() => {\r\n            this.isLoading = false\r\n          })\r\n    }\r\n\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.doctor-page-table {\r\n  padding: 20px;\r\n}\r\n\r\n.loading-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  height: 50px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.el-button.is-disabled {\r\n  cursor: not-allowed;\r\n}\r\n</style>"],"mappings":"AA4BA,SAAAA,SAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,QAAA;MACAC,SAAA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;IACA;EACA;EAEAC,QAAA;IACA,MAAAC,MAAA,QAAAN,IAAA,CAAAO,EAAA;IACA,KAAAC,SAAA,CAAAF,MAAA;EACA;EACAG,OAAA;IAGA,MAAAD,UAAAF,MAAA;MACA,KAAAI,QAAA,CAAAC,GAAA,qBAAAL,MAAA,IACAM,IAAA,CAAAC,QAAA;QACA,IAAAA,QAAA,CAAAhB,IAAA;UACA,KAAAC,QAAA,GAAAe,QAAA,CAAAhB,IAAA;QACA;UACA,KAAAiB,WAAA,eAAAD,QAAA,CAAAhB,IAAA;QACA;MACA,GACAkB,KAAA,CAAAC,KAAA;QACA,KAAAF,WAAA,eAAAE,KAAA;MACA,GACAC,OAAA;QACA,KAAAlB,SAAA;MACA;IACA;EAGA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}